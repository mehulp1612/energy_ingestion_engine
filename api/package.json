{
  "name": "energy-ingestion-api",
  "version": "1.0.0",
  "description": "High-scale energy ingestion engine",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate src/database/migrations/InitSchema -d src/database/typeorm.datasource.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/database/typeorm.datasource.ts"

  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/typeorm": "^10.0.0",
    "typeorm": "^0.3.20",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "ts-node": "^10.9.2"
  }
}
